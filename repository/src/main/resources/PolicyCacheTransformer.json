      "actions": ["add-relationship"]
    }
  ],
  "atlan-remove-terms": [
    {
      "category": "relationship",
      "resources": [
        "relationship-type:*",

        "end-one-entity-type:AtlasGlossaryTerm",
        "end-one-entity-classification:*",
        "end-one-entity:*",

        "end-two-entity-type:*",
        "end-two-entity-classification:*",
        "end-two-entity:{entity}"
      ],
      "actions": ["remove-relationship"]
    }
  ]
